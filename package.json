{
  "name": "@automattic/vip-js-sdk",
  "description": "JavaScript library for the WPVIP REST API",
  "version": "0.0.1",
  "author": "Automattic, Inc.",
  "repository": "git@github.com:Automattic/vip-js-sdk.git",
  "license": "MIT",
  "scripts": {
    "cmd:format": "prettier '**/*.(js|json|jsx|md|ts|tsx|yml|yaml)'",
    "cmd:lint": "eslint --ext 'js,jsx,ts,tsx' --quiet",
    "format": "npm run cmd:format -- --write",
    "format:check": "npm run cmd:format -- --check",
    "lint": "npm run cmd:lint .",
    "lint:fix": "npm run cmd:lint . -- --fix",
    "test": "npm run lint"
  },
  "dependencies": {
    "superagent": "3.8.3",
    "superagent-proxy": "3.0.0"
  },
  "devDependencies": {
    "@automattic/eslint-plugin-wpvip": "0.5.8",
    "eslint": "8.44.0",
    "prettier": "npm:wp-prettier@2.8.5"
  }
}
